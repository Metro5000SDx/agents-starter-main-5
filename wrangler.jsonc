{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "agents-starter-main-5",
  "main": "src/server.ts",
  "compatibility_date": "2025-08-03",
  "compatibility_flags": ["nodejs_compat"],
    "ai": {
      "binding": "AI",
      "remote": true
    },
    "vars": {
      "AI_GATEWAY_ID": "llm-chat-app"
    },
  "assets": {
    "directory": "public"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "Chat",
        "class_name": "Chat"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["Chat"]
    }
  ]
}
